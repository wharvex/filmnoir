<%-include('partials/head', {pageTitle})%>
<%-include('partials/navbar', { secPic: secPics[0], activeArr: ["", "active", "", ""] })%>
<div class="container mb-5">
    <div class="row mt-3 pb-4 d-flex justify-content-evenly border-bottom border-dark">
        <div class="col-sm-5 text-center">
            <h1 class="display-1"><%=pageTitle%></h1>
        </div>
    </div>
    <div class="row d-flex justify-content-evenly">
        <%for (let i=0; i<themeData.length; i++) {%>
        <%if (i>2 && i%3===0) {%>
        <div class="row d-flex justify-content-evenly">
            <%-include('partials/themecardpic', {i})%>
            <%} else if (i>1 && (i+1)%3===0) {%>
            <%-include('partials/themecardpic', {i})%>
        </div>
        <%} else {%>
        <%-include('partials/themecardpic', {i})%>
        <%}%>
        <%}%>
    </div>
</div>
<%-include('partials/footer', { secPic1: secPics[1], secPic2: secPics[2] })%>