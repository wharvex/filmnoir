<%- include('partials/head', { pageTitle }) %>
<%- include('partials/navbar', { secPic: secPics[0], activeArr: ["active", "", "", ""] }) %>
<div class="container">
    <%- include('partials/titleboxscene', { pageTitle: pageTitle, mainPic: mainPic }) %>
    <div class="row mt-4">
        <% for (let i=0; i<filmData.length; i++) { %>
        <% if (i>2 && i%3===0) { %>
        <div class="row mt-4">
            <%- include('partials/filmlinkcol', { i }) %>
            <% } else if (i>1 && (i+1)%3===0) { %>
            <%- include('partials/filmlinkcol', { i }) %>
        </div>
        <% } else { %>
        <%- include('partials/filmlinkcol', { i }) %>
        <% } %>
        <% } %>
    </div>
</div>
<%- include('partials/footer', { secPic1: secPics[1], secPic2: secPics[2] }) %>